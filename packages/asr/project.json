{
  "name": "asr",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/asr/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsup",
        "cwd": "packages/asr"
      },
      "outputs": ["{workspaceRoot}/packages/asr/dist"]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run --coverage.enabled=false",
        "cwd": "packages/asr"
      },
      "outputs": ["{workspaceRoot}/coverage"]
    },
    "test:watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest",
        "cwd": "packages/asr"
      }
    },
    "test:coverage": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run --coverage",
        "cwd": "packages/asr"
      },
      "outputs": ["{workspaceRoot}/coverage"]
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "packages/asr"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "biome check packages/asr"
      }
    },
    "lint:fix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "biome check --write packages/asr"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsup --watch",
        "cwd": "packages/asr"
      }
    }
  },
  "tags": ["type:lib", "scope:asr"]
}
