{
  "name": "shared-types",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/shared-types/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsup",
        "cwd": "packages/shared-types"
      },
      "outputs": ["{workspaceRoot}/dist/shared-types"]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run --config packages/shared-types/vitest.config.ts"
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "packages/shared-types"
      }
    }
  },
  "tags": ["type:lib"]
}
