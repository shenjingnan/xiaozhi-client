{
  "name": "@xiaozhi-client/version",
  "version": "2.0.0-beta.3",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/shenjingnan/xiaozhi-client.git",
    "directory": "packages/version"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^24.3.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "tsup": "^8.3.5"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "{projectRoot}/dist"
        ]
      },
      "test": {
        "outputs": [
          "{workspaceRoot}/coverage/version"
        ]
      }
    }
  }
}
