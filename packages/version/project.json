{
  "name": "version",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/version/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsup",
        "cwd": "packages/version"
      },
      "outputs": ["{workspaceRoot}/dist/version"]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run --config packages/version/vitest.config.ts"
      },
      "outputs": ["{workspaceRoot}/coverage/version"]
    },
    "test:watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest --config packages/version/vitest.config.ts"
      }
    },
    "test:coverage": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run --coverage --config packages/version/vitest.config.ts"
      },
      "outputs": ["{workspaceRoot}/coverage/version"]
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "packages/version"
      }
    },
    "nx-release-publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm publish --access public --no-git-checks",
        "cwd": "packages/version"
      }
    }
  },
  "tags": ["type:lib", "scope:version"]
}
