{
  "name": "@xiaozhi-client/config",
  "version": "1.10.9",
  "description": "小智客户端配置管理模块，提供统一的配置读取和管理功能",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/shenjingnan/xiaozhi-client.git",
    "directory": "packages/config"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "comment-json": "^4.2.5",
    "dayjs": "^1.11.13"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
