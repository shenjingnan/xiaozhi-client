{
  "name": "@xiaozhi-client/tts",
  "version": "0.0.1",
  "description": "@xiaozhi-client/tts",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run --coverage.enabled=false",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "check:type": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "demo": "tsx examples/demo.ts",
    "demo:stream": "tsx examples/demo-stream.ts",
    "demo:convenience": "tsx examples/demo-convenience.ts",
    "check:spell": "cspell .",
    "check:cpd": "jscpd . --ignore 'node_modules'",
    "check:all": "pnpm run lint && pnpm run check:type && pnpm run check:spell && pnpm run check:cpd",
    "clean:dist": "rimraf dist"
  },
  "keywords": [
    "tts",
    "websocket",
    "bytedance",
    "streaming",
    "speech-synthesis"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ws": "^8.16.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/ws": "^8.5.10",
    "dotenv": "^16.4.0",
    "tsup": "^8.5.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b"
}
