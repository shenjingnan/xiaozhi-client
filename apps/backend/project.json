{
  "name": "backend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/backend",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run build",
        "cwd": "apps/backend",
        "env": {
          "PWD": "{workspaceRoot}"
        }
      },
      "outputs": ["{workspaceRoot}/dist/backend"],
      "dependsOn": ["shared-types:build", "config:build"]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run test",
        "cwd": "apps/backend"
      },
      "outputs": ["{workspaceRoot}/coverage"]
    },
    "test:watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run test:watch",
        "cwd": "apps/backend"
      }
    },
    "test:coverage": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run test:coverage",
        "cwd": "apps/backend"
      },
      "outputs": ["{workspaceRoot}/coverage"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run lint",
        "cwd": "apps/backend"
      }
    },
    "lint:fix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run lint:fix",
        "cwd": "apps/backend"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run format",
        "cwd": "apps/backend"
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run check:type",
        "cwd": "apps/backend"
      },
      "dependsOn": ["config:build", "endpoint:build", "version:build"]
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run dev",
        "cwd": "apps/backend",
        "env": {
          "PWD": "{workspaceRoot}"
        }
      }
    }
  },
  "tags": ["type:app", "platform:node"],
  "implicitDependencies": ["shared-types"]
}
