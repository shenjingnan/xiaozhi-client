{
  "name": "backend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/backend",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter backend run build"
      },
      "outputs": ["{workspaceRoot}/dist/backend"],
      "dependsOn": ["shared-types:build", "config:build"]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter backend run test"
      },
      "outputs": ["{workspaceRoot}/coverage"]
    },
    "test:watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter backend run test:watch"
      }
    },
    "test:coverage": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter backend run test:coverage"
      },
      "outputs": ["{workspaceRoot}/coverage"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter backend run lint"
      }
    },
    "lint:fix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter backend run lint:fix"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter backend run format"
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter backend run check:type"
      },
      "dependsOn": ["config:build", "endpoint:build", "version:build"]
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter backend run dev"
      }
    }
  },
  "tags": ["type:app", "platform:node"],
  "implicitDependencies": ["shared-types"]
}
